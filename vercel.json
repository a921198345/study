{
  "version": 2,
  "github": {
    "enabled": true,
    "silent": true
  },
  "buildCommand": "npm install --include=dev && npm run build",
  "installCommand": "npm install --include=dev",
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "study-cuiges-projects.vercel.app"
        }
      ],
      "destination": "https://study-sage-chi.vercel.app/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "study-rabx98gev-cuiges-projects.vercel.app"
        }
      ],
      "destination": "https://study-sage-chi.vercel.app/:path*",
      "permanent": true
    }
  ]
}
